class Solution:
    def numJewelsInStones(self, jewels: str, stones: str) -> int:
        count=0
        for i in stones:
            for j in jewels:
                if i==j:
                    count+=1
        return (count)

#other way and faster way is 

class Solution:
    def numJewelsInStones(self, jewels: str, stones: str) -> int:
        x=list(jewels)
        count1=0
        for i in stones:
            y=x.count(i)
            count1+=y
        return (count1)
